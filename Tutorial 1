        "\n",
        "# Class 2\n",
        "# r = np.random.normal(mean, SD, no_points)\n",
        "r1 = np.random.normal(5, 1, 200)\n",
        "# theta = np.random.uniform(lower,upper,number)\n",
        "theta1 = np.random.uniform(0,2*np.pi,200)\n",
        "\n",
        "x1 = r * np.cos(theta)\n",
        "y1 = r * np.sin(theta)\n",
        "\n",
        "plt.scatter(x1,y1)\n",
        "\n",
        "# Class 1\n",
        "\n",
        "r2 = np.random.normal(2, 1, 100)\n",
        "theta2 = np.random.uniform(0,2*np.pi,100)\n",
        "\n",
        "r3 = np.random.normal(8, 1, 400)\n",
        "theta3 = np.random.uniform(0,2*np.pi,400)\n",
        "\n",
        "r4 = np.concatenate((r2, r3))\n",
        "theta4 = np.concatenate((theta2, theta3))\n",
        "\n",
        "x2 = r4 * np.cos(theta4)\n",
        "y2 = r4 * np.sin(theta4)\n",
        "\n",
        "plt.scatter(x2,y2)\n",
        "\n",
        "\n"
